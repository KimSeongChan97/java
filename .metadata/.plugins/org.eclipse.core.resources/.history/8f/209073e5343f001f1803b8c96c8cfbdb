package class_;


public class SungJuk {
	private String name;
	private int kor, eng, math, tot;
	private double avg;
	private char grade;
	
	public void setData(String n, int k, int e, int m) { // 구현 시뮬
		name = n;
		kor = k;
		eng = e;
		math = m;
	}
	
	public void calc() {
		tot = kor + eng + math;
		avg = avg / 3.;
		if(avg>=90) grade = 'A';
		else if(avg>=80) grade = 'B';
		else if(avg>=70) grade = 'C';
		else if(avg>=60) grade = 'D';
		else grade = 'F' ;
	}
	
	public void getName() {
		
	}
	
	
	
}
