package for_;

public class For02 {

	public static void main(String[] args) {
		int i;
		for(i = 2; i*9; i++) {
			System.out.println(" 2 * i = " + i);
		}
		System.out.println();
	}

}



/*

    i 
2 * 1 = 2
2 * 2 = 4
2 * 3 = 6
2 * 4 = 8
2 * 5 = 10
2 * 6 = 12
2 * 7 = 14
2 * 8 = 16
2 * 9 = 18


*/