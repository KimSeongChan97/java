package for_;

public class For01 {

	public static void main(String[] args) {
		/* for(A;B;c) {
			D;
			}
			E   //A->B(조건) -> [D -> C -> B(조건)] 반복. -> 거짓일 때 E
		*/
		
		System.out.println("Hello Kitty!!");
		System.out.println("Hello Kitty!!");
		System.out.println("Hello Kitty!!");
		System.out.println("Hello Kitty!!");
		System.out.println("Hello Kitty!!"); // 단순한 반복
		
		// int i; // 지역변수 i (local variable)
		for(int i = 1; i<=5; i++) {
			System.out.println("Hello Kitty!!");
		} // i가 5번 반복
		
		
		
		
		
		
		
		
		

	}

}
